{
  "name": "ionic-react-seed",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@aws-amplify/ui-react": "^0.2.34",
    "@capacitor/core": "2.4.6",
    "@ionic/react": "^5.0.7",
    "@ionic/react-router": "^5.0.7",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.3",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.24",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "aws-amplify": "^3.3.14",
    "bulma": "^0.9.2",
    "ionicons": "^5.0.0",
    "path": "^0.12.7",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "typescript": "3.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "firebase deploy",
    "buildndeploy": "npm run gulp && react-scripts build && npm run deploy",
    "gulp": "gulp && npm run src",
    "src": "prettier --write src/App.tsx && prettier --write src/components/** && prettier --write src/user-components/*.tsx && prettier --write src/user-pages/*.tsx",
    "create-front-api": "gulp -f gulpcmd/cmd-rename-package-name.js && gulp -f gulpsh/sh-frontapi-git-clone.js && gulp -f gulpcmd/cmd-rename-project.js && gulp -f gulpsh/sh-frontapi-yarn.js && gulp -f gulpsh/sh-frontapi-deploy.js && gulp -f gulpcmd/cmd-pull-endpoint.js",
    "api-update": "gulp -f gulpcmd/cmd-api-update.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "create-sample": "gulp -f gulpfile-createsample.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "2.4.6",
    "chalk": "^4.1.0",
    "exec-sh": "^0.3.4",
    "gulp": "^4.0.2",
    "lodash": "^4.17.20",
    "prettier": "^2.2.1"
  },
  "description": "An Ionic + React seed project"
}
