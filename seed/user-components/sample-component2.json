{
  "name": "SampleComponent2",
  "import": [
    {
      "name": "IonCard",
      "from": "@ionic/react",
      "type": "package",
      "props": []
    },
    {
      "name": "IonCardHeader",
      "from": "@ionic/react",
      "type": "package",
      "props": []
    },
    {
      "name": "IonCardSubtitle",
      "from": "@ionic/react",
      "type": "package",
      "props": []
    },
    {
      "name": "IonCardTitle",
      "from": "@ionic/react",
      "type": "package",
      "props": []
    },
    {
      "name": "IonCardContent",
      "from": "@ionic/react",
      "type": "package",
      "props": []
    },
    {
      "name": "IonButton",
      "from": "@ionic/react",
      "type": "package",
      "props": []
    },
    {
      "name": "IonModal",
      "from": "@ionic/react",
      "type": "package",
      "props": []
    },
    {
      "name": "* as fetch",
      "from": "../components/fetch",
      "type": "default",
      "props": []
    }
  ],
  "state": [
    {
      "name": "modal",
      "params": [
        {
          "name": "isOpen",
          "type": "boolean",
          "init": "false"
        }
      ]
    }
  ],
  "fetch": [
    {
      "format": "post",
      "name": "postMessage",
      "args": "message: string",
      "codeFirst": "console.log('This is it.')",
      "codeLast": "this.setState({modal: {isOpen: true}})",
      "apis": [
        {
          "api": "https://api.mocklets.com/mock68049/update1",
          "postTypeName": "bodyPostMessage",
          "postType": "msg: string",
          "postBody": "{msg: message}",
          "responseTypeName": "resultPostMessage",
          "responseType": "result: string"
        }
      ]
    }
  ],
  "tags": [
    {
      "tag": "div",
      "child": {
        "tags": [
          {
            "tag": "IonCard",
            "props": [],
            "child": {
              "tags": [
                {
                  "tag": "IonCardHeader",
                  "props": [],
                  "child": {
                    "tags": [
                      {
                        "tag": "IonCardTitle",
                        "noCR": "yes",
                        "props": [],
                        "content": "Sample Card Title"
                      },
                      {
                        "tag": "IonCardSubtitle",
                        "noCR": "yes",
                        "props": [],
                        "content": "Sample Card Subtitle"
                      }
                    ]
                  }
                },
                {
                  "tag": "IonCardContent",
                  "props": [],
                  "content": "Keep close to Nature's heart... and break clear away, once in awhile, and climb a mountain or spend a week in the woods. Wash your spirit clean."
                }
              ]
            }
          },
          {
            "tag": "IonCard",
            "props": [],
            "child": {
              "tags": [
                {
                  "tag": "IonCardHeader",
                  "props": [],
                  "child": {
                    "tags": [
                      {
                        "tag": "IonCardTitle",
                        "noCR": "yes",
                        "props": [],
                        "content": "Sample Button"
                      },
                      {
                        "tag": "IonCardSubtitle",
                        "noCR": "yes",
                        "props": [],
                        "content": "This is POST button"
                      }
                    ]
                  }
                },
                {
                  "tag": "IonCardHeader",
                  "props": [],
                  "child": {
                    "tags": [
                      {
                        "tag": "IonModal",
                        "props": ["isOpen={this.state.modal.isOpen}"],
                        "child": {
                          "tags": [
                            {
                              "tag": "p",
                              "content": "This is modal content"
                            },
                            {
                              "tag": "IonButton",
                              "props": ["onClick={() => this.setState({modal: {isOpen: false}})}"],
                              "content": "Close Modal"
                            }
                          ]
                        }
                      },
                      {
                        "tag": "IonButton",
                        "props": ["shape=\"round\"", "fill=\"outline\"", "expand=\"full\"", "onClick={ () => this.postMessage(\"HOGE\")}"],
                        "content": "Button"
                      }
                    ]
                  }
                }
              ]
            }
          }
        ]
      }
    }
  ]
}
